{
  "name": "@randajan/simple-app",
  "version": "6.2.5",
  "description": "Supersimple start for developing full stack (express + frontend) app with node.js",
  "repository": "randajan/simple-lib",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./be": {
      "import": "./dist/esm/be/index.mjs",
      "require": "./dist/cjs/be/index.cjs"
    },
    "./be/server": {
      "import": "./dist/esm/be/server.mjs",
      "require": "./dist/cjs/be/server.cjs"
    },
    "./be/express": {
      "import": "./dist/esm/be/express.mjs",
      "require": "./dist/cjs/be/express.cjs"
    },
    "./be/koa": {
      "import": "./dist/esm/be/koa.mjs",
      "require": "./dist/cjs/be/koa.cjs"
    },
    "./fe": {
      "import": "./dist/esm/fe/index.mjs",
      "require": "./dist/cjs/fe/index.cjs"
    },
    "./info": {
      "import": "./dist/esm/uni/info.mjs",
      "require": "./dist/cjs/uni/info.cjs"
    },
    "./log": {
      "import": "./dist/esm/uni/log.mjs",
      "require": "./dist/cjs/uni/log.cjs"
    },
    "./env": {
      "import": "./dist/esm/uni/env.mjs",
      "require": "./dist/cjs/uni/env.cjs"
    },
    "./argv": {
      "import": "./dist/esm/uni/argv.mjs",
      "require": "./dist/cjs/uni/argv.cjs"
    },
    "./fs": {
      "import": "./dist/esm/uni/fs.mjs",
      "require": "./dist/cjs/uni/fs.cjs"
    },
    "./inject": {
      "import": "./dist/esm/uni/inject.mjs",
      "require": "./dist/cjs/uni/inject.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "node build.js",
    "test": "node test.js",
    "testWorker": "node testWorker.js"
  },
  "dependencies": {
    "@randajan/std-io": "^1.0.4",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "detect-port": "^2.1.0",
    "esbuild": "^0.25.1",
    "esbuild-node-externals": "^1.18.0",
    "fs-extra": "^11.2.0",
    "open": "^10.1.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@randajan/simple-lib": "^3.0.2",
    "express": "^4.21.2",
    "koa": "^2.16.0",
    "koa-static": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "esbuild",
    "web",
    "boilerplate",
    "simple",
    "dev",
    "express",
    "chokidar",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/randajan/simple-app",
  "bugs": {
    "url": "https://github.com/randajan/simple-app/issues"
  },
  "author": {
    "name": "Jan Randa",
    "email": "jnranda@gmail.com",
    "url": "https://www.linkedin.com/in/randajan/"
  },
  "peerDependencies": {}
}
